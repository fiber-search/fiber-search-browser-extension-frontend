{
    "manifest_version": 2,
    "name": "Fiber Search Recorder",
    "version": "0.0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "16": "img/logo.png",
        "48": "img/logo.png",
        "128": "img/logo.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "img/logo.png",
            "38": "img/logo.png"
        },
        "default_title": "Fiber Search Recorder"
    }
}